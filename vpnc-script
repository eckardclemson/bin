#!/bin/sh

DISTRO_SCRIPT=/usr/share/vpnc-scripts/vpnc-script

if [ ! -f $DISTRO_SCRIPT ]; then
        DISTRO_SCRIPT=/etc/vpnc/vpnc-script
fi

CISCO_SPLIT_INC=2 \
CISCO_SPLIT_INC_0_ADDR=130.127.0.0 \
CISCO_SPLIT_INC_0_MASK=255.255.0.0 \
CISCO_SPLIT_INC_0_MASKLEN=16 \
CISCO_SPLIT_INC_0_PROTOCOL=0 \
CISCO_SPLIT_INC_1_ADDR=10.0.0.0 \
CISCO_SPLIT_INC_1_MASK=255.0.0.0 \
CISCO_SPLIT_INC_1_MASKLEN=8 \
CISCO_SPLIT_INC_1_PROTOCOL=0 \
CISCO_SPLIT_INC_2_ADDR=192.168.8.0 \
CISCO_SPLIT_INC_2_MASK=255.255.255.0 \
CISCO_SPLIT_INC_2_MASKLEN=24 \
CISCO_SPLIT_INC_2_PROTOCOL=0 \
CISCO_SPLIT_INC_3_ADDR=192.168.9.0 \
CISCO_SPLIT_INC_3_MASK=255.255.255.0 \
CISCO_SPLIT_INC_3_MASKLEN=24 \
CISCO_SPLIT_INC_3_PROTOCOL=0 \
CISCO_SPLIT_INC_4_ADDR=192.168.10.0 \
CISCO_SPLIT_INC_4_MASK=255.255.255.0 \
CISCO_SPLIT_INC_4_MASKLEN=24 \
CISCO_SPLIT_INC_4_PROTOCOL=0 \
$DISTRO_SCRIPT
