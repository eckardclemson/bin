# Script to create symlinks of the files in this repo in ~/bin

# TODO:  check if there already, prompt to overwrite
ln -s ${PWD}/cuvpn ${HOME}/bin
ln -s ${PWD}/cuvpn ${HOME}/bin/cudrvpn
ln -s ${PWD}/vpnc-script ${HOME}/bin
ln -s ${PWD}/rdesktop.vdi ${HOME}/bin
ln -s ${PWD}/spectrum ${HOME}/bin
ln -s ${PWD}/vmware-view.vdi ${HOME}/bin
pushd ${HOME}/bin
ln -s rdesktop.vdi vdi.rdesktop
ln -s vmware-view.vdi vdi.vmware-view
popd
